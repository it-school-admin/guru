<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/screen/window.xsd"
        caption="msg://eductaionalYearEdit.caption"
        focusComponent="form"
        messagesPack="su.itschool.guru.web.screens.eductaionalyear">
    <data>
        <instance id="eductaionalYearDc"
                  class="su.itschool.guru.entity.EductaionalYear">
            <view extends="_local"/>
            <loader/>
        </instance>
    </data>
    <dialogMode height="600"
                width="800"/>
    <layout expand="editActions" spacing="true">
        <form id="form" dataContainer="eductaionalYearDc">
            <column width="350px">
                <textField id="nameField" property="name"/>
                <dateField id="startDateField" property="startDate"/>
                <dateField id="endDateField" property="endDate"/>
            </column>
        </form>
        <hbox id="editActions" spacing="true">
            <button id="commitAndCloseBtn" action="windowCommitAndClose"/>
            <button id="closeBtn" action="windowClose"/>
        </hbox>
    </layout>
</window>
