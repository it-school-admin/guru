        <buttonsPanel>
            <button id="selectAllBtn" caption="msg://selectAllBtn.caption"/>
            <button id="unselectAllClasses" caption="msg://unselectAllClasses.caption"/>
        </buttonsPanel>
        <grid id="classesCheckBoxesGrid" spacing="true">
